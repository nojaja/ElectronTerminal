{
  "name": "terminal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build-win": "vue-cli-service electron:build --win --x64 --publish never",
    "electron:build-mac": "vue-cli-service electron:build --mac --x64 --publish never",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "author": "nojaja",
  "license": "MIT",
  "main": "background.js",
  "dependencies": {
    "@vue/cli": "^4.3.1",
    "core-js": "^2.6.11",
    "fix-path": "^2.1.0",
    "iconv-lite": "^0.4.24",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue": "^2.6.11",
    "xterm": "^3.14.5",
    "xterm-addon-fit": "^0.1.0",
    "xterm-addon-web-links": "^0.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "babel-eslint": "^10.1.0",
    "electron": "15.5.5",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "vue-cli-plugin-electron-builder": "^1.4.6",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
